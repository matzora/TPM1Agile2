--TEST--
"for" tag can use an "else" clause
--TEMPLATE--
{% for item in items %}
  * {{ item }}
{% else %}
  no item
{% endfor %}
--DATA--
return array('items' => array('a', 'a'))
--EXPECT--
  * a
  * a
--DATA--
return array('items' => array())
--EXPECT--
  no item
--DATA--
return array()
--CONFIG--
return array('strict_variables' => false)
--EXPECT--
  no item
